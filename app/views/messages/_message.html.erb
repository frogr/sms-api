<div id="<%= dom_id message %>">
  <p>
    <strong>To number:</strong>
    <%= message.to_number %>
  </p>

  <p>
    <strong>Callback url:</strong>
    <%= message.callback_url %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= message.message %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= message.status %>
  </p>

</div>
